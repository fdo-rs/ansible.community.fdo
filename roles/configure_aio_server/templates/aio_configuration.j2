cert_organization: {{ aio_configuration_cert_organization }}
cert_country: {{ aio_configuration_cert_country }}
listen_ip_address: {{ aio_configuration_listen_ip_address }}
listen_port_manufacturing_server: {{ aio_configuration_listen_port_manufacturing_server }}
listen_port_owner_onboarding_server: {{ aio_configuration_listen_port_owner_onboarding_server }}
listen_port_rendezvous_server: {{ aio_configuration_listen_port_rendezvous_server }}
