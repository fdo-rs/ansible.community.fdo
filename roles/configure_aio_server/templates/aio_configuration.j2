separate_manufacturing_and_owner_voucher_store: {{ aio_configuration_separate_manufacturing_and_owner_voucher_store | bool | lower }}
manufacturing_enable_plain_di: {{ aio_configuration_manufacturing_enable_plain_di | bool | lower }}
manufacturing_disable_key_storage_filesystem: {{ aio_configuration_manufacturing_disable_key_storage_filesystem | bool | lower }}
manufacturing_disable_key_storage_tpm: {{ aio_configuration_manufacturing_disable_key_storage_tpm | bool | lower }}
manufacturing_use_secp256r1: {{ aio_configuration_manufacturing_use_secp256r1 | bool | lower }}
contact_hostname: {{ aio_configuration_contact_hostname }}
